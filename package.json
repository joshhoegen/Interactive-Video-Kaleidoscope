    "prettier-eslint-cli": "^4.7.1",
    "pretty-quick": "^1.6.0",
    "react-transform-hmr": "^1.0.4",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
